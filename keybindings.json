[
  {
    "key": "ctrl+alt+w",
    "command": "editor.emmet.action.wrapWithAbbreviation"
  },
  {
    "key": "alt+v",
    "command": "toggleVim"
  },
  {
    "key": "ctrl+alt+z",
    "command": "workbench.action.toggleZenMode",
    "when": "!isAuxiliaryWindowFocusedContext"
  },
  {
    "key": "ctrl+k z",
    "command": "-workbench.action.toggleZenMode",
    "when": "!isAuxiliaryWindowFocusedContext"
  },
  {
    "key": "ctrl+alt+c",
    "command": "workbench.action.toggleCenteredLayout"
  }
]
